{
  "name": "nova-faas-webui",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start:server": "docker-compose exec platform-web node_modules/.bin/nodemon --config nodemon.json server",
    "test:standard": "node_modules/.bin/standard --verbose | snazzy",
    "test:coverage": "./node_modules/.bin/nyc --reporter=lcov --reporter=text ./node_modules/.bin/mocha --opts .mocharc",
    "coverage:publish": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gperreymond/nova-faas-webui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gperreymond/nova-faas-webui/issues"
  },
  "homepage": "https://github.com/gperreymond/nova-faas-webui#readme",
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "debug": "3.1.0",
    "hapi": "16.6.2",
    "joi": "13.0.2",
    "nova-faas": "1.0.2",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chance": "1.0.12",
    "coveralls": "3.0.0",
    "mocha": "4.0.1",
    "mocha-standard": "1.0.0",
    "nodemon": "1.12.1",
    "nyc": "11.3.0",
    "rewire": "3.0.2",
    "sinon": "4.1.2",
    "snazzy": "7.0.0",
    "standard": "10.0.3"
  },
  "standard": {
    "globals": [
      "xit",
      "beforeEach",
      "describe",
      "it"
    ],
    "ignore": [
      "coverage/**"
    ]
  }
}
